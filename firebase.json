{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites":[
      {
        "source": "/api/**",
        "function": "api"
      }
    ]
  },
  "emulators": {
    "hosting": {
      "host": "127.0.0.1",
      "port": "8001"
    }
  }
}
